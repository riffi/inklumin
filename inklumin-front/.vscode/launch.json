{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Run Yarn Script: dev",
            "runtimeExecutable": "node",
            "args": [
                "${workspaceFolder}/.yarn/releases/yarn-4.8.1.cjs",
                "run",
                "dev"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        }
    ]
}